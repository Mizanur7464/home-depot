{
  "name": "homedepot-clearance-website",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:server": "node scripts/kill-port.js && tsx watch server/index.ts",
    "dev:server:safe": "tsx watch server/index.ts",
    "kill-port": "node scripts/kill-port.js",
    "add-test-data": "tsx scripts/add-test-data.ts",
    "add-clearance-test": "tsx scripts/add-clearance-test-data.ts",
    "remove-clearance-test": "tsx scripts/remove-clearance-test.ts",
    "refresh-clearance": "tsx scripts/refresh-without-auth.ts",
    "remove-test-data": "tsx scripts/remove-test-data.ts",
    "remove-duplicates": "tsx scripts/remove-duplicates.ts",
    "remove-non-clearance": "tsx scripts/remove-non-clearance.ts",
    "clear-old-errors": "tsx scripts/clear-old-errors.ts",
    "cleanup-old-products": "tsx scripts/cleanup-old-products.ts",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "docker:build": "docker-compose build",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f",
    "docker:dev": "docker-compose -f docker-compose.dev.yml up -d",
    "pm2:start": "pm2 start ecosystem.config.js --env production",
    "pm2:stop": "pm2 stop ecosystem.config.js",
    "pm2:restart": "pm2 restart ecosystem.config.js",
    "pm2:delete": "pm2 delete ecosystem.config.js"
  },
  "dependencies": {
    "axios": "^1.6.5",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^9.0.0",
    "next": "^14.2.0",
    "node-cron": "^3.0.3",
    "playwright": "^1.41.1",
    "react": "^18.3.0",
    "react-dom": "^18.3.0",
    "redis": "^4.6.12"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^20.11.0",
    "@types/node-cron": "^3.0.11",
    "@types/react": "^18.3.0",
    "@types/react-dom": "^18.3.0",
    "tsx": "^4.7.1",
    "typescript": "^5.3.3"
  }
}
